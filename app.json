{
  "expo": {
    "name": "Stuff",
    "slug": "stuff-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img_login.png",
      "resizeMode": "cover",
      "backgroundColor": "#F5A452"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5A452"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cadub.stuffmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "80a750c6-abb5-4b33-afe7-9c41ac6cd46e"
      }
    },
    "plugins": [
      "expo-asset",
      "react-native-edge-to-edge"
    ]
  }
}
